{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@ai-chat/ai-chat/(.*)": "<rootDir>/../libs/ai-chat/src/$1",
    "@ai-chat/ai-chat": "<rootDir>/../libs/ai-chat/src",
    "@app/token/(.*)": "<rootDir>/../libs/token/src/$1",
    "@app/token": "<rootDir>/../libs/token/src",
    "@analysis/analysis/(.*)": "<rootDir>/../libs/analysis/src/$1",
    "@analysis/analysis": "<rootDir>/../libs/analysis/src",
    "Y/parser/(.*)": "<rootDir>/../libs/parser/src/$1",
    "Y/parser": "<rootDir>/../libs/parser/src",
    "@database/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@database/database": "<rootDir>/../libs/database/src"
  }
}